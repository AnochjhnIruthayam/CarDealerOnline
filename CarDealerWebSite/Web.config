<configuration>
    <system.web>
      <compilation debug="true" strict="false" explicit="true" targetFramework="4.0" />
    </system.web>
  <appSettings>
    <add key="provider" value="System.Data.SqlClient"/>
  </appSettings>
  <connectionStrings>
    <add name="CarDealerSQLProvider" connectionString="Data Source = (local)\SQLEXPRESS; Integrated Security = SSPI; Initial Catalog = CarDealerDB"/>
  </connectionStrings>

</configuration>
